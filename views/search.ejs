<% title="Search Results" %>
<div class="tweetArea">
    <h2 class="title">Search Results - &quot;<%= searchString %>&quot;</h2>
    <% tweets.forEach(function(tweet){ %>
        <div class='tweet'>
            <div class="username"><a href="/user/<%= tweet.author.username %>"><%= tweet.author.username %></a></div>
            <div class="content"><%= tweet.content %></div>
        </div>
    <% }) %>
</div>